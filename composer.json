{
    "name": "vagnerkaefer/nfse-nacional",
    "description": "Biblioteca PHP para integração com o Sistema Nacional de Nota Fiscal de Serviço Eletrônica (NFS-e)",
    "type": "library",
    "keywords": [
        "nfse",
        "nota fiscal",
        "fiscal",
        "nfs-e",
        "sistema nacional",
        "sefin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vagner Kaefer",
            "email": "vagner@kaefer.eng.br"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "tecnickcom/tcpdf": "^6.10"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "NFSe\\": "src/NFSe/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NFSe\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist"
    }
}
